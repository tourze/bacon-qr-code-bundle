{
  "name": "tourze/bacon-qr-code-bundle",
  "description": "Symfony Bundle for bacon/bacon-qr-code",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-filter": "*",
    "bacon/bacon-qr-code": "^3.0",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/http-foundation": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/routing": "^6.4",
    "symfony/twig-bundle": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "tourze/bundle-dependency": "0.0.*",
    "tourze/symfony-integration-test-kernel": "0.0.*",
    "tourze/symfony-routing-auto-loader-bundle": "0.0.*",
    "twig/twig": "^3.13|^4.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "BaconQrCodeBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BaconQrCodeBundle\\Tests\\": "tests"
    }
  }
}